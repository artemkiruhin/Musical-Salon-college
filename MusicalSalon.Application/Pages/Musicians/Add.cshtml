@page
@model MusicalSalon.Application.Pages.Musicians.AddModel

<h2>Create Musician</h2>

<form method="post">
    <div class="form-group">
        <label asp-for="Musician.Name"></label>
        <input asp-for="Musician.Name" class="form-control" />
        <span asp-validation-for="Musician.Name" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Musician.CareerYear"></label>
        <input asp-for="Musician.CareerYear" class="form-control" />
        <span asp-validation-for="Musician.CareerYear" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Create</button>
</form>
